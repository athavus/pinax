{
    "version": 1,
    "bindings": [
        {
            "key": "mod+p",
            "command": "quickSearch.open",
            "description": "Open quick repository search"
        },
        {
            "key": "mod+u",
            "command": "repository.push",
            "description": "Push commits to remote"
        },
        {
            "key": "mod+shift+p",
            "command": "repository.pull",
            "description": "Pull commits from remote"
        },
        {
            "key": "mod+r",
            "command": "repository.refresh",
            "description": "Refresh current repository status"
        },
        {
            "key": "mod+shift+e",
            "command": "sidebar.focus",
            "description": "Focus the sidebar"
        },
        {
            "key": "mod+e",
            "command": "editor.open",
            "description": "Open current repository in default editor"
        },
        {
            "key": "mod+t",
            "command": "terminal.open",
            "description": "Open current repository in default terminal"
        },
        {
            "key": "mod+d",
            "command": "fileExplorer.open",
            "description": "Open current repository in file explorer"
        },
        {
            "key": "mod+h",
            "command": "shortcuts.show",
            "description": "Show all commands"
        }
    ]
}