{
    "version": 1,
    "bindings": [
        {
            "key": "mod+p",
            "command": "repository.push",
            "description": "Push commits to remote"
        },
        {
            "key": "mod+shift+p",
            "command": "repository.pull",
            "description": "Pull commits from remote"
        },
        {
            "key": "mod+r",
            "command": "repository.refresh",
            "description": "Refresh current repository status"
        },
        {
            "key": "mod+shift+e",
            "command": "sidebar.focus",
            "description": "Focus the sidebar"
        },
        {
            "key": "mod+1",
            "command": "workspace.select.1",
            "description": "Select first workspace"
        },
        {
            "key": "mod+2",
            "command": "workspace.select.2",
            "description": "Select second workspace"
        },
        {
            "key": "mod+3",
            "command": "workspace.select.3",
            "description": "Select third workspace"
        }
    ]
}